{
  "name": "web-app",
  "version": "1.0.1",
  "description": "",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "scripts": {
    "prepublish": "npm run build",
    "start:dev": "run-p start:dev:*",
    "start:dev:http": "http-server ./dev",
    "start-:dev:watch": "watch 'npm run build:ts' ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf ./dist && tsc",
    "dev:build": "run-s dev:build:*",
    "dev:build:ts": "browserify --full-paths -r todo-apis -r ./src/index.tsx:init --debug -t [envlocalify --envfile=../shared/public.env ] -t browserify-css -p [ tsify -p ./tsconfig.json ] > ./dev/hidden.build.js && echo \"finished build\"",
    "dev:build:ugly": "terser ./dev/hidden.build.js -o ./dev/hidden.ugly.js --compress --mangle --sourcemap \"filename='./dev/hidden.ugly.min.js.map',url='hidden.ugly.min.js.map'\"",
    "dev:build:disc": "discify ./dev/hidden.build.js > ./dev/hidden.stats.html",
    "dev:build:disc-ugly": "discify ./dev/hidden.ugly.js > ./dev/hidden.ugly-stats.html"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/redux-logger": "^3.0.7",
    "browserify": "^16.5.0",
    "browserify-css": "^0.15.0",
    "chokidar-cli": "^2.0.0",
    "disc": "^1.3.3",
    "envify": "^4.1.0",
    "envlocalify": "^1.1.1",
    "foreach-cli": "^1.8.1",
    "http-server": "^0.11.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "terser": "^4.3.8",
    "tinyify": "^2.5.1",
    "tsify": "^4.0.1",
    "typescript": "^3.6.2",
    "uglify-js": "^3.6.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "todo-apis": "file:../api"
  }
}
