    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "browserify": "^16.5.0",
    "browserify-css": "^0.15.0",
    "foreach-cli": "^1.8.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "require-solidity": "github:formula1/require-solidity",
    "rimraf": "^3.0.0",
    "solc": "^0.5.11",
    "tsify": "^4.0.1",
    "typescript": "^3.6.2",
    "uglify-js": "^3.6.0"
  },
  "dependencies": {
    "@types/redux-logger": "^3.0.7",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.24.0",
    "mongodb": "^3.3.2",
    "observable": "^2.1.4",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "web3": "^1.2.1"
  }
