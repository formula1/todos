{
  "name": "ui",
  "version": "1.0.0",
  "description": "",
  "main": "util.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "run-s build:*",
    "build:ts": "browserify ./src/index.tsx --debug -t require-solidity -t browserify-css -p [ tsify -p ./tsconfig.json ] > ./dist/hidden.build.js",
    "build:ugly": "uglifyjs ./dist/hidden.build.js  --source-map -o ./dist/hidden.ugly.js",
    "sol": "run-s sol:*",
    "sol:clean": "rimraf ./dist/ethereum",
    "sol:build": "run-s sol:build:*",
    "sol:build:init": "mkdirp ./dist/ethereum",
    "sol:build:echo": "foreach -g \"./src/todo/api/ethereum/contracts/*\" -x \"echo \\\"#{path} #{base}\\\"\"",
    "sol:build:each": "solcjs --bin --abi -o ./dist/ethereum ./src/todo/api/ethereum/contracts/arrayops.sol ./src/todo/api/ethereum/contracts/todo.sol"
  },
  "author": "",
