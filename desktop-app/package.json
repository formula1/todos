    "electron-builder": "^21.2.0",
    "envlocalify": "^1.1.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "^3.2.0",
    "require-solidity": "^1.0.3",
    "rimraf": "^3.0.0",
